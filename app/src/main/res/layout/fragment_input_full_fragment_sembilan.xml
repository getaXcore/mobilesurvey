                                android:textColor="@android:color/black" />

                            <View
                                android:layout_alignParentBottom="true"
                                android:id="@+id/garis_balance_terakhir_di_rekening_tabungan"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_below="@+id/etx_balance_terakhir_di_rekening_tabungan"
                                android:layout_toRightOf="@+id/gambar_balance_terakhir_di_rekening_tabungan"
                                android:background="#eaeaea"></View>

                        </RelativeLayout>





                        <RelativeLayout
                            android:id="@+id/box_rata_rata_mutasi_in_3_bulan_terakhir"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/box_balance_terakhir_di_rekening_tabungan"
                            android:layout_margin="5dp">


                            <ImageView
                                android:layout_alignParentTop="true"
                                android:id="@+id/gambar_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginRight="10dp"
                                android:src="@drawable/icon_rata_rata_mutasi_in" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:textSize="11dp"
                                android:textColor="@color/bg_hed_ce"
                                android:id="@+id/t_judul_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:layout_toRightOf="@+id/gambar_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:text="@string/rata_rata_mutasi_in_3_bulan_terakhir"/>

                            <EditText
                                android:layout_below="@+id/t_judul_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:textSize="14dp"
                                android:id="@+id/etx_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="number"
                                android:layout_toRightOf="@+id/gambar_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:background="@android:color/transparent"
                                android:textColor="@android:color/black" />

                            <View
                                android:layout_alignParentBottom="true"
                                android:id="@+id/garis_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_below="@+id/etx_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:layout_toRightOf="@+id/gambar_rata_rata_mutasi_in_3_bulan_terakhir"
                                android:background="#eaeaea"></View>

                        </RelativeLayout>


                        <RelativeLayout
                            android:id="@+id/box_rata_rata_mutasi_out_3_bulan_terakhir"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/box_rata_rata_mutasi_in_3_bulan_terakhir"
                            android:layout_margin="5dp">


                            <ImageView
                                android:layout_alignParentTop="true"
                                android:id="@+id/gambar_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginRight="10dp"
                                android:src="@drawable/icon_rata_rata_mutasi_out" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:textSize="11dp"
                                android:textColor="@color/bg_hed_ce"
                                android:id="@+id/t_judul_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:layout_toRightOf="@+id/gambar_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:text="@string/rata_rata_mutasi_out_3_bulan_terakhir"/>

                            <EditText
                                android:layout_below="@+id/t_judul_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:textSize="14dp"
                                android:id="@+id/etx_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="number"
                                android:layout_toRightOf="@+id/gambar_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:background="@android:color/transparent"
                                android:textColor="@android:color/black" />

                            <View
                                android:layout_alignParentBottom="true"

                                android:src="@drawable/icon_total_cicilan" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:textSize="11dp"
                                android:textColor="@color/bg_hed_ce"
                                android:id="@+id/t_judul_total_cicilan_leasing_lain"
                                android:layout_toRightOf="@+id/gambar_total_cicilan_leasing_lain"
                                android:text="@string/total_cicilan_leasing_lain"/>

                            <EditText
                                android:layout_below="@+id/t_judul_total_cicilan_leasing_lain"
                                android:textSize="14dp"
                                android:id="@+id/etx_total_cicilan_leasing_lain"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="number"
                                android:layout_toRightOf="@+id/gambar_total_cicilan_leasing_lain"
                                android:background="@android:color/transparent"
                                android:textColor="@android:color/black" />

                            <View
                                android:layout_alignParentBottom="true"
                                android:id="@+id/garis_total_cicilan_leasing_lain"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_below="@+id/etx_total_cicilan_leasing_lain"
                                android:layout_toRightOf="@+id/gambar_total_cicilan_leasing_lain"
                                android:background="#eaeaea"></View>

                        </RelativeLayout>




                        <RelativeLayout
                            android:id="@+id/box_balance_terakhir_di_rekening_tabungan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/box_total_cicilan_leasing_lain"
                            android:layout_margin="5dp">


                            <ImageView
                                android:layout_alignParentTop="true"
                                android:id="@+id/gambar_balance_terakhir_di_rekening_tabungan"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginRight="10dp"
                                android:src="@drawable/icon_balance_terakhir_di_rekening_tabungan" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:textSize="11dp"
                                android:textColor="@color/bg_hed_ce"
                                android:id="@+id/t_judul_balance_terakhir_di_rekening_tabungan"
                                android:layout_toRightOf="@+id/gambar_balance_terakhir_di_rekening_tabungan"
                                android:text="@string/balance_terakhir_di_rekening_tabungan"/>

                            <EditText
                                android:layout_below="@+id/t_judul_balance_terakhir_di_rekening_tabungan"
                                android:textSize="14dp"
                                android:id="@+id/etx_balance_terakhir_di_rekening_tabungan"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="number"
                                android:layout_toRightOf="@+id/gambar_balance_terakhir_di_rekening_tabungan"

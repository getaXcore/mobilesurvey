                                android:layout_height="30dp"
                                android:layout_marginRight="10dp"
                                android:src="@drawable/icon_pengeluaran_or_kebutuhan_hidup" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:textSize="11dp"
                                android:textColor="@color/bg_hed_ce"
                                android:id="@+id/t_judul_pengeluaran_or_kebutuhan_hidup"
                                android:layout_toRightOf="@+id/gambar_pengeluaran_or_kebutuhan_hidup"
                                android:text="@string/pengeluaran_or_kebutuhan_hidup"/>

                            <EditText
                                android:layout_below="@+id/t_judul_pengeluaran_or_kebutuhan_hidup"
                                android:textSize="14dp"
                                android:id="@+id/etx_pengeluaran_or_kebutuhan_hidup"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:inputType="number"
                                android:layout_toRightOf="@+id/gambar_pengeluaran_or_kebutuhan_hidup"
                                android:background="@android:color/transparent"
                                android:textColor="@android:color/black" />

                            <View
                                android:layout_alignParentBottom="true"
                                android:id="@+id/garis_pengeluaran_or_kebutuhan_hidup"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_below="@+id/etx_pengeluaran_or_kebutuhan_hidup"
                                android:layout_toRightOf="@+id/gambar_pengeluaran_or_kebutuhan_hidup"
                                android:background="#eaeaea"></View>

                        </RelativeLayout>


                        <RelativeLayout
                            android:id="@+id/box_total_cicilan_leasing_lain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/box_pengeluaran_or_kebutuhan_hidup"
                            android:layout_margin="5dp">


                            <ImageView
                                android:layout_alignParentTop="true"
                                android:id="@+id/gambar_total_cicilan_leasing_lain"

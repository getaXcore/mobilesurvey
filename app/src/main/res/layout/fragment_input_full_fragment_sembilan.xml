                                android:layout_below="@+id/etx_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:layout_toRightOf="@+id/gambar_rata_rata_mutasi_out_3_bulan_terakhir"
                                android:background="#eaeaea"></View>

                        </RelativeLayout>




                    </RelativeLayout>
                </RelativeLayout>

            </RelativeLayout>



            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/bt_simpan"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/data_pendapatan_or_kapasitas"
                android:textColor="@color/putih"
                android:background="@color/biru_muda"
                android:text="@string/bt_simpan"/>

        </RelativeLayout>
    </ScrollView>





</RelativeLayout>
